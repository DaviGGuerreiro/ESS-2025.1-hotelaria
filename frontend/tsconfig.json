{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["cypress", "node"]
  },
  "include": [
    "src",
    "cypress/**/*.ts",
    "cypress/**/*.js"
  ]
}
